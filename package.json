{
  "name": "ibkr-statements-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@nivo/core": "^0.88.0",
    "@nivo/line": "^0.88.0",
    "@nivo/sankey": "^0.88.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-csv-reader": "^4.0.0",
    "react-dom": "^19.0.0",
    "react-json-view": "^1.21.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env ESLINT_NO_DEV_ERRORS=true react-scripts start",
    "build": "react-scripts build",
    "build:ci": "CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,css,md}\"",
    "deps:check": "depcheck --ignores=\"@testing-library/*,web-vitals,react-scripts,@nivo/core,eslint-config-react-app\" --ignore-patterns=\"build/*,coverage/*,public/*\"",
    "code-quality": "npm run lint && npm run format:check",
    "code-quality:fix": "npm run lint:fix && npm run format"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(d3|d3-array|d3-scale|d3-interpolate|d3-color|d3-format|d3-time|d3-time-format|d3-selection|d3-transition|d3-ease|d3-timer|d3-dispatch|d3-drag|d3-brush|d3-zoom|internmap|delaunator|robust-predicates|@nivo)/)"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "cross-env": "7.0.3",
    "depcheck": "^1.4.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2"
  },
  "resolutions": {
    "nth-check": ">=2.0.1",
    "postcss": ">=8.4.31",
    "webpack-dev-server": "^4.15.2"
  }
}
